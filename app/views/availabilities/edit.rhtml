<% content_for "page_title" do -%>
  Editing availability
<% end -%>
<%= form_tag :action => 'edit', :id => @availability do -%>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Save' %>
<% end -%>
<%= button_to 'Destroy', { :action => 'destroy', :id => @availability }, :confirm => 'Are you sure you want to destroy this availability?' %>

<%= link_to 'Show', :action => 'show', :id => @availability %> |
<%= link_to 'Back to list', :action => 'index' %>
