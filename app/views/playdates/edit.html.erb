<% content_for :page_title do %>
  Editing playdate
<% end %>

<%= form_for @playdate do %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Save' %>
<% end %>
<%= button_to 'Destroy', { :action => 'destroy', :id => @playdate },
  :method => :delete,
  :confirm => 'Are you sure you want to destroy this playdate?' %>

<%= link_to 'Show', :action => 'show', :id => @playdate %> |
<%= link_to 'Back to list', :action => 'index' %>
