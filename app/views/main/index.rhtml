<h1>Playdate! The Application</h1>

<table>
  <tr>
    <th></th>
    <th>Playdate</th>
  </tr>
  
  <tr>
    <th>Player</th>
    <% for playdate in @playdates %>
    <td><%=h playdate.day %></td>
    <% end %>
  </tr>
  <% for player in regular_players %>
  <tr>
    <td><%=h player.abbreviation %></td>
  </tr>
  <% end %>
</table>
